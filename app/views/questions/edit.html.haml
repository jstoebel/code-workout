= render partial: 'layouts/breadcrumb'

%h1 Edit Question
%h2
    =@question.title

=render(:partial => 'application/record_error_messages', :locals => {:record => @question})

= form_for(@question, url: question_path(@question.id)) do |f|

    =render partial: 'form', :locals => {:f => f}
    = submit_tag("Submit")
